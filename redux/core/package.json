{
  "name": "@metabuilder/redux-core",
  "version": "1.0.0",
  "description": "Core Redux slices for MetaBuilder - shared state management across all frontends (Next.js, Qt6, CLI)",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "homepage": "https://github.com/metabuilder/metabuilder",
  "repository": {
    "type": "git",
    "url": "https://github.com/metabuilder/metabuilder.git",
    "directory": "redux/core"
  },
  "author": "MetaBuilder Dev Team",
  "license": "MIT",
  "keywords": [
    "redux",
    "state-management",
    "react",
    "metabuilder",
    "workflow",
    "authentication",
    "projects",
    "workspaces"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "echo 'Test suite coming soon'",
    "lint": "echo 'Lint configured in root'"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "react": "^19.2.4",
    "react-redux": "^9.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-redux": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./slices": {
      "types": "./src/slices/index.ts",
      "import": "./src/slices/index.ts",
      "default": "./src/slices/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "import": "./src/types/index.ts",
      "default": "./src/types/index.ts"
    },
    "./store": {
      "types": "./src/store/index.ts",
      "import": "./src/store/index.ts",
      "default": "./src/store/index.ts"
    },
    "./middleware": {
      "types": "./src/middleware/index.ts",
      "import": "./src/middleware/index.ts",
      "default": "./src/middleware/index.ts"
    }
  },
  "files": [
    "src",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
