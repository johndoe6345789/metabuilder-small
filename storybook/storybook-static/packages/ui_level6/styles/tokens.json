{
  "$schema": "https://metabuilder.dev/schemas/package-styles.schema.json",
  "schemaVersion": "2.0.0",
  "colors": {
    "supergodPrimary": "#d32f2f",
    "supergodSecondary": "#ff5722",
    "supergodBackground": "#1a1a1a",
    "supergodSurface": "#2d2d2d",
    "supergodText": "#ffffff",
    "supergodAccent": "#ffc107",
    "tenantActive": "#4caf50",
    "tenantInactive": "#9e9e9e",
    "transferWarning": "#ff9800",
    "systemHealthy": "#28a745",
    "systemUnhealthy": "#dc3545",
    "maintenanceMode": "#ff5722"
  },
  "spacing": {
    "panelPadding": "24px",
    "sidebarWidth": "280px",
    "toolbarHeight": "64px",
    "cardGap": "16px",
    "formSpacing": "16px"
  },
  "shadows": {
    "panel": "0 4px 12px rgba(0, 0, 0, 0.3)",
    "card": "0 2px 8px rgba(0, 0, 0, 0.15)",
    "toolbar": "0 2px 4px rgba(0, 0, 0, 0.2)"
  },
  "borders": {
    "panelRadius": "8px",
    "cardRadius": "4px",
    "inputRadius": "4px"
  }
}
