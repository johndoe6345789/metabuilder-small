{
  "$schema": "https://metabuilder.dev/schemas/package-styles.schema.json",
  "schemaVersion": "2.0.0",
  "colors": {
    "kvSuccess": "#28a745",
    "kvError": "#dc3545",
    "blobPrimary": "#0066cc",
    "blobSecondary": "#6c757d",
    "cacheHit": "#28a745",
    "cacheMiss": "#ffc107",
    "connectionActive": "#28a745",
    "connectionInactive": "#dc3545"
  },
  "spacing": {
    "panelPadding": "16px",
    "cardGap": "16px",
    "buttonGroupGap": "8px",
