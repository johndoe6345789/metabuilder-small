  "shadows": {
    "panel": "0 2px 8px rgba(0, 0, 0, 0.1)",
    "card": "0 1px 4px rgba(0, 0, 0, 0.08)",
    "resultPanel": "0 2px 4px rgba(0, 0, 0, 0.06)"
