{
  "name": "meta.workflow_packages.contextual_iterative_loop.label",
  "active": false,
  "nodes": [
    {
      "id": "list_files",
      "name": "List Files",
      "type": "tools.list_files",
      "typeVersion": 1,
      "position": [
        100,
        100
      ],
      "parameters": {
        "path": "."
      }
    },
    {
      "id": "filter_python",
      "name": "Filter Python",
      "type": "utils.filter_list",
      "typeVersion": 1,
      "position": [
        400,
        100
      ],
      "parameters": {
        "items": "$repo_files",
        "mode": "regex",
        "pattern": "\\.py$"
      }
    },
    {
      "id": "map_python",
      "name": "Map Python",
      "type": "utils.map_list",
      "typeVersion": 1,
      "position": [
        700,
        100
      ],
      "parameters": {
        "items": "$python_files",
        "template": "PY: {item}"
      }
    },
    {
      "id": "reduce_python",
      "name": "Reduce Python",
      "type": "utils.reduce_list",
      "typeVersion": 1,
      "position": [
        100,
        300
      ],
      "parameters": {
        "items": "$python_lines",
        "separator": "\\n"
      }
    },
    {
      "id": "seed_messages",
      "name": "Seed Messages",
      "type": "core.seed_messages",
      "typeVersion": 1,
      "position": [
        400,
        300
      ],
      "parameters": {}
    },
    {
      "id": "append_repo_summary",
      "name": "Append Repo Summary",
      "type": "core.append_context_message",
      "typeVersion": 1,
      "position": [
        700,
        300
      ],
      "parameters": {
        "messages": "$messages",
        "context": "$python_summary"
      }
    },
    {
      "id": "append_user_instruction",
      "name": "Append User Instruction",
      "type": "core.append_user_instruction",
      "typeVersion": 1,
      "position": [
        100,
        500
      ],
      "parameters": {
        "messages": "$messages"
      }
    },
    {
      "id": "main_loop",
      "name": "Main Loop",
      "type": "control.loop",
      "typeVersion": 1,
      "position": [
        400,
        500
      ],
      "parameters": {
        "max_iterations": 5,
        "stop_when": "$no_tool_calls",
        "stop_on": "true"
      }
    }
  ],
