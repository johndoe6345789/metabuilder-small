{
  "name": "workflowui",
  "version": "1.0.0",
  "type": "module",
  "description": "Visual workflow editor UI - Modern n8n-like interface for MetaBuilder workflows",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --webpack || true",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:init": "node scripts/init-db.js",
    "db:migrate": "node scripts/migrate-db.js",
    "backend": "python backend/server.py",
    "dev:all": "concurrently \"npm run dev\" \"npm run backend\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@metabuilder/api-clients": "file:../../redux/api-clients",
    "@metabuilder/components": "file:../../components",
    "@metabuilder/core-hooks": "file:../../redux/core-hooks",
    "@metabuilder/fakemui": "file:../../components/fakemui",
    "@metabuilder/hooks": "file:../../hooks",
    "@metabuilder/hooks-data": "file:../../redux/hooks-data",
    "@metabuilder/redux-middleware": "file:../../redux/middleware",
    "@metabuilder/redux-persist": "*",
    "@metabuilder/redux-slices": "file:../../redux/slices",
    "@metabuilder/service-adapters": "file:../../redux/adapters",
    "@metabuilder/services": "file:../../redux/services",
    "@metabuilder/translations": "file:../../translations",
    "@metabuilder/types": "file:../../types",
    "@reduxjs/toolkit": "^2.11.2",
    "axios": "^1.13.5",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.3.0",
    "framer-motion": "^12.34.3",
    "lodash-es": "^4.17.23",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.8.3",
    "uuid": "^13.0.0"
  },
  "overrides": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@storybook/react": "^10.2.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "typescript": "~5.9.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "@chromatic-com/playwright": "^0.12.8",
    "checkly": "^7.1.0",
    "npm-run-all2": "8.0.4",
    "@sentry/nextjs": "^10.39.0",
    "@spotlightjs/spotlight": "^4.10.0"
  }
}
