{
  "id": "test-typescript-math",
  "name": "TypeScript Math Workflow",
  "description": "Simple math workflow: add two numbers, multiply by 2, return result",
  "nodes": [
    {
      "id": "trigger_manual_1",
      "type": "trigger.manual",
      "name": "Start",
      "position": { "x": 100, "y": 200 },
      "config": {
        "initialData": {
          "numberA": 5,
          "numberB": 3
        }
      },
      "inputs": [],
      "outputs": ["main"]
    },
    {
      "id": "math_add_1",
      "type": "math.add",
