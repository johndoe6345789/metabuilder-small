      "inputs": ["main"],
      "outputs": ["main"]
    },
    {
      "id": "math_multiply_1",
      "type": "math.multiply",
      "name": "Multiply by 2",
      "position": { "x": 600, "y": 200 },
      "config": {
        "a": "{{$input.main}}",
        "b": 2
      },
      "inputs": ["main"],
      "outputs": ["main"]
    }
  ],
  "connections": [
    {
      "id": "conn_1",
      "sourceNodeId": "trigger_manual_1",
      "sourceOutput": "main",
      "targetNodeId": "math_add_1",
      "targetInput": "main"
    },
    {
      "id": "conn_2",
      "sourceNodeId": "math_add_1",
      "sourceOutput": "main",
      "targetNodeId": "math_multiply_1",
      "targetInput": "main"
    }
  ],
  "expectedOutput": 16,
  "expectedNodeResults": {
    "trigger_manual_1": {
      "numberA": 5,
      "numberB": 3
    },
    "math_add_1": 8,
    "math_multiply_1": 16
  }
