{
  "id": "tips-card",
  "type": "div",
  "props": {
    "className": "tips-card"
  },
  "children": [
    {
      "id": "tips-card-header",
      "type": "div",
      "props": {
        "className": "tips-card__header"
      },
      "children": [
        {
          "type": "Icon",
          "props": {
            "name": "warning",
            "weight": "duotone",
            "size": 24,
            "className": "tips-card__icon"
          }
        },
        {
          "id": "tips-card-title",
          "type": "div",
          "props": {
            "className": "tips-card__title"
          },
          "children": [
            {
              "type": "Text",
              "props": {
                "content": "Quick Tips"
              }
            }
          ]
        }
      ]
    },
    {
      "id": "tips-card-list",
      "type": "div",
      "props": {
        "className": "tips-card__list"
      },
      "children": [
        {
          "type": "forEach",
          "source": "tips",
          "condition": "item.show",
          "children": [
            {
              "type": "div",
              "props": {
                "className": "tips-card__item"
              },
              "children": [
                {
                  "type": "Text",
                  "bindings": {
                    "content": "â€¢ {item.message}"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
