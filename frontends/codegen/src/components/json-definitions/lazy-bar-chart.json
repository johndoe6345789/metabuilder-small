{
  "id": "lazy-bar-chart",
  "type": "div",
  "props": {
    "className": "chart-container chart-container--bar"
  },
  "bindings": {
    "style": {
      "source": "height",
      "transform": "data ? { height: typeof data === 'number' ? data + 'px' : data } : {}"
    }
  },
  "children": [
    {
      "id": "lazy-bar-chart-placeholder",
      "type": "div",
      "props": {
        "className": "chart-container__placeholder"
      },
      "children": [
        {
          "id": "lazy-bar-chart-text",
          "type": "span",
          "children": "Bar Chart (D3 required)"
        }
      ]
    }
  ]
}
