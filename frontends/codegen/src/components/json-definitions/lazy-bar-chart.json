{
  "id": "lazy-bar-chart",
  "type": "div",
  "props": {
    "className": "w-full"
  },
  "bindings": {
    "style": {
      "source": "height",
      "transform": "data ? { height: typeof data === 'number' ? data + 'px' : data } : { height: '300px' }"
    }
  },
  "children": [
    {
      "id": "lazy-bar-chart-placeholder",
      "type": "div",
      "props": {
        "className": "flex items-center justify-center h-full text-muted-foreground bg-muted/30 rounded-md"
      },
      "children": [
        {
          "id": "lazy-bar-chart-text",
          "type": "span",
          "props": {
            "className": "text-sm"
          },
          "children": "Bar Chart (D3 required)"
        }
      ]
    }
  ]
}
