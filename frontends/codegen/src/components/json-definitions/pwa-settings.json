                            "variant": {
                              "source": "pwaState.hasRegistration",
                              "transform": "data ? 'default' : 'secondary'"
                            },
                            "children": {
                              "source": "pwaState.hasRegistration",
                              "transform": "data ? pwaState.copy.cache.status.active : pwaState.copy.cache.status.inactive"
                            }
                          }
                        }
                      ]
                    }
                  ]
                },
                {
                  "id": "cache-separator-bottom",
                  "type": "Separator"
                },
                {
                  "id": "cache-clear-button",
                  "type": "Button",
                  "props": {
                    "variant": "destructive",
                    "className": "w-full"
                  },
                  "bindings": {
                    "onClick": "pwaState.handleClearCache"
                  },
                  "children": [
                    {
                      "id": "cache-clear-icon",
                      "type": "Trash",
                      "props": {
                        "size": 16,
                        "className": "mr-2"
                      }
                    },
                    {
                      "id": "cache-clear-text",
                      "type": "span",
                      "bindings": {
                        "children": "pwaState.copy.cache.action.clear"
                      }
                    }
                  ]
                },
                {
                  "id": "cache-helper-text",
                  "type": "p",
                  "props": {
                    "className": "text-xs text-muted-foreground text-center"
                  },
                  "bindings": {
                    "children": "pwaState.copy.cache.helper"
                  }
                }
              ]
            }
          ]
        },
