                          },
                          "bindings": {
                            "children": "pwaState.copy.features.items.pushNotifications"
                          }
                        },
                        {
                          "id": "feature-push-notifications-check",
                          "type": "CheckCircle",
                          "conditional": {
                            "if": "pwaState.isNotificationSupported"
                          },
                          "props": {
                            "size": 16,
                            "className": "text-accent",
                            "weight": "fill"
                          }
                        },
                        {
                          "id": "feature-push-notifications-x",
                          "type": "XCircle",
                          "conditional": {
                            "if": "!pwaState.isNotificationSupported"
                          },
                          "props": {
                            "size": 16,
                            "className": "text-muted-foreground",
                            "weight": "fill"
                          }
                        }
                      ]
                    },
                    {
                      "id": "feature-shortcuts",
                      "type": "div",
                      "props": {
                        "className": "flex items-center justify-between"
                      },
                      "children": [
                        {
                          "id": "feature-shortcuts-label",
                          "type": "span",
                          "props": {
                            "className": "text-muted-foreground"
                          },
                          "bindings": {
                            "children": "pwaState.copy.features.items.shortcuts"
                          }
                        },
                        {
                          "id": "feature-shortcuts-icon",
                          "type": "CheckCircle",
                          "props": {
                            "size": 16,
                            "className": "text-accent",
                            "weight": "fill"
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
