{
  "id": "pwa-settings-container",
  "type": "div",
  "props": {
    "className": "h-full overflow-auto p-6 space-y-6"
  },
  "children": [
    {
      "id": "pwa-settings-header",
      "type": "div",
      "children": [
        {
          "id": "pwa-settings-title",
          "type": "h2",
          "props": {
            "className": "text-2xl font-bold mb-2"
          },
          "bindings": {
            "children": "pwaState.copy.header.title"
          }
        },
        {
          "id": "pwa-settings-description",
          "type": "p",
          "props": {
            "className": "text-sm text-muted-foreground"
          },
          "bindings": {
            "children": "pwaState.copy.header.description"
          }
        }
      ]
    },
    {
      "id": "pwa-settings-grid",
      "type": "div",
      "props": {
        "className": "grid gap-6"
      },
      "children": [
        {
          "id": "install-section",
          "type": "Card",
          "props": {
            "className": "p-6"
          },
          "children": [
            {
              "id": "install-section-inner",
              "type": "div",
              "props": {
                "className": "space-y-4"
              },
              "children": [
                {
                  "id": "install-header",
                  "type": "div",
                  "children": [
                    {
                      "id": "install-title",
                      "type": "h3",
                      "props": {
                        "className": "text-lg font-semibold mb-1"
                      },
                      "bindings": {
                        "children": "pwaState.copy.install.title"
                      }
                    },
                    {
                      "id": "install-description",
                      "type": "p",
                      "props": {
                        "className": "text-sm text-muted-foreground"
                      },
                      "bindings": {
                        "children": "pwaState.copy.install.description"
                      }
                    }
                  ]
                },
                {
                  "id": "install-content",
                  "type": "div",
                  "props": {
                    "className": "flex items-center justify-between"
                  },
                  "children": [
                    {
                      "id": "install-info",
                      "type": "div",
                      "props": {
                        "className": "flex items-center gap-3"
                      },
                      "children": [
                        {
                          "id": "install-icon",
                          "type": "Download",
                          "props": {
                            "size": 20,
                            "className": "text-muted-foreground"
                          }
                        },
                        {
                          "id": "install-labels",
                          "type": "div",
                          "children": [
                            {
                              "id": "install-label",
                              "type": "Label",
                              "props": {
                                "className": "text-base"
                              },
                              "bindings": {
                                "children": "pwaState.copy.install.label"
                              }
                            },
                            {
                              "id": "install-status-text",
                              "type": "p",
                              "props": {
                                "className": "text-xs text-muted-foreground"
                              },
                              "bindings": {
                                "children": {
                                  "source": "pwaState.isInstalled,pwaState.isInstallable",
                                  "transform": "pwaState.isInstalled ? pwaState.copy.install.status.installed : pwaState.isInstallable ? pwaState.copy.install.status.available : pwaState.copy.install.status.notAvailable"
                                }
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "id": "install-actions",
                      "type": "div",
                      "props": {
                        "className": "flex items-center gap-2"
                      },
                      "children": [
                        {
                          "id": "install-badge-installed",
                          "type": "Badge",
                          "conditional": {
                            "if": "pwaState.isInstalled"
                          },
                          "props": {
                            "variant": "default"
                          },
                          "bindings": {
                            "children": "pwaState.copy.install.badge.installed"
                          }
                        },
                        {
                          "id": "install-button",
                          "type": "Button",
                          "conditional": {
                            "if": "pwaState.isInstallable && !pwaState.isInstalled"
                          },
                          "props": {
                            "size": "sm"
                          },
                          "bindings": {
                            "onClick": "pwaState.handleInstall",
                            "children": "pwaState.copy.install.action.install"
                          }
                        },
                        {
                          "id": "install-badge-unavailable",
                          "type": "Badge",
                          "conditional": {
                            "if": "!pwaState.isInstallable && !pwaState.isInstalled"
                          },
                          "props": {
                            "variant": "secondary"
                          },
                          "bindings": {
                            "children": "pwaState.copy.install.badge.notAvailable"
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "id": "connection-section",
          "type": "Card",
          "props": {
            "className": "p-6"
          },
          "children": [
            {
              "id": "connection-inner",
              "type": "div",
              "props": {
                "className": "space-y-4"
              },
              "children": [
                {
                  "id": "connection-header",
                  "type": "div",
                  "children": [
                    {
                      "id": "connection-title",
                      "type": "h3",
                      "props": {
                        "className": "text-lg font-semibold mb-1"
                      },
                      "bindings": {
                        "children": "pwaState.copy.connection.title"
                      }
                    },
                    {
                      "id": "connection-description",
                      "type": "p",
                      "props": {
                        "className": "text-sm text-muted-foreground"
                      },
                      "bindings": {
                        "children": "pwaState.copy.connection.description"
                      }
                    }
                  ]
                },
                {
                  "id": "connection-content",
                  "type": "div",
                  "props": {
                    "className": "flex items-center justify-between"
                  },
                  "children": [
                    {
                      "id": "connection-info",
                      "type": "div",
                      "props": {
                        "className": "flex items-center gap-3"
                      },
                      "children": [
                        {
                          "id": "connection-icon-online",
                          "type": "WifiHigh",
                          "conditional": {
                            "if": "pwaState.isOnline"
                          },
                          "props": {
                            "size": 20,
                            "className": "text-accent"
                          }
                        },
                        {
                          "id": "connection-icon-offline",
                          "type": "WifiSlash",
                          "conditional": {
                            "if": "!pwaState.isOnline"
                          },
                          "props": {
                            "size": 20,
                            "className": "text-destructive"
                          }
                        },
                        {
                          "id": "connection-labels",
                          "type": "div",
                          "children": [
                            {
                              "id": "connection-label",
                              "type": "Label",
                              "props": {
                                "className": "text-base"
                              },
                              "bindings": {
                                "children": "pwaState.copy.connection.label"
                              }
                            },
                            {
                              "id": "connection-status-text",
                              "type": "p",
                              "props": {
                                "className": "text-xs text-muted-foreground"
                              },
                              "bindings": {
                                "children": {
                                  "source": "pwaState.isOnline",
                                  "transform": "data ? pwaState.copy.connection.status.online : pwaState.copy.connection.status.offline"
                                }
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "id": "connection-badge",
                      "type": "Badge",
                      "bindings": {
                        "variant": {
                          "source": "pwaState.isOnline",
                          "transform": "data ? 'default' : 'destructive'"
                        },
                        "children": {
                          "source": "pwaState.isOnline",
                          "transform": "data ? pwaState.copy.connection.badge.online : pwaState.copy.connection.badge.offline"
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "id": "update-section",
          "type": "div",
          "conditional": {
            "if": "pwaState.isUpdateAvailable"
          },
          "children": [
            {
              "id": "update-card",
              "type": "Card",
              "props": {
                "className": "p-6 border-accent"
              },
              "children": [
                {
                  "id": "update-inner",
                  "type": "div",
                  "props": {
                    "className": "space-y-4"
                  },
                  "children": [
                    {
                      "id": "update-header",
                      "type": "div",
                      "children": [
                        {
                          "id": "update-title",
                          "type": "h3",
                          "props": {
                            "className": "text-lg font-semibold mb-1"
                          },
                          "bindings": {
                            "children": "pwaState.copy.update.title"
                          }
                        },
                        {
                          "id": "update-description",
                          "type": "p",
                          "props": {
                            "className": "text-sm text-muted-foreground"
                          },
                          "bindings": {
                            "children": "pwaState.copy.update.description"
                          }
                        }
                      ]
                    },
                    {
                      "id": "update-content",
                      "type": "div",
                      "props": {
                        "className": "flex items-center justify-between"
                      },
                      "children": [
                        {
                          "id": "update-info",
                          "type": "div",
                          "props": {
                            "className": "flex items-center gap-3"
                          },
                          "children": [
                            {
                              "id": "update-icon",
                              "type": "CloudArrowDown",
                              "props": {
                                "size": 20,
                                "className": "text-accent"
                              }
                            },
                            {
                              "id": "update-labels",
                              "type": "div",
                              "children": [
                                {
                                  "id": "update-label",
                                  "type": "Label",
                                  "props": {
                                    "className": "text-base"
                                  },
                                  "bindings": {
                                    "children": "pwaState.copy.update.label"
                                  }
                                },
                                {
                                  "id": "update-status-text",
                                  "type": "p",
                                  "props": {
                                    "className": "text-xs text-muted-foreground"
                                  },
                                  "bindings": {
                                    "children": "pwaState.copy.update.status"
                                  }
                                }
                              ]
                            }
                          ]
                        },
                        {
                          "id": "update-button",
                          "type": "Button",
                          "bindings": {
                            "onClick": "pwaState.handleUpdate",
                            "children": "pwaState.copy.update.action"
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "id": "notifications-section",
          "type": "Card",
          "props": {
            "className": "p-6"
          },
          "children": [
            {
              "id": "notifications-inner",
              "type": "div",
              "props": {
                "className": "space-y-4"
              },
              "children": [
                {
                  "id": "notifications-header",
                  "type": "div",
                  "children": [
                    {
                      "id": "notifications-title",
                      "type": "h3",
                      "props": {
                        "className": "text-lg font-semibold mb-1"
                      },
                      "bindings": {
                        "children": "pwaState.copy.notifications.title"
                      }
                    },
                    {
                      "id": "notifications-description",
                      "type": "p",
                      "props": {
                        "className": "text-sm text-muted-foreground"
                      },
                      "bindings": {
                        "children": "pwaState.copy.notifications.description"
                      }
                    }
                  ]
                },
                {
                  "id": "notifications-content",
                  "type": "div",
                  "props": {
                    "className": "flex items-center justify-between"
                  },
                  "children": [
                    {
                      "id": "notifications-info",
                      "type": "div",
                      "props": {
                        "className": "flex items-center gap-3"
                      },
                      "children": [
                        {
                          "id": "notifications-icon",
                          "type": "Bell",
                          "props": {
                            "size": 20,
                            "className": "text-muted-foreground"
                          }
                        },
                        {
                          "id": "notifications-labels",
                          "type": "div",
                          "children": [
                            {
                              "id": "notifications-label",
                              "type": "Label",
                              "props": {
                                "className": "text-base"
                              },
                              "bindings": {
                                "children": "pwaState.copy.notifications.label"
                              }
                            },
                            {
                              "id": "notifications-permission-row",
                              "type": "div",
                              "props": {
                                "className": "flex items-center gap-2"
                              },
                              "children": [
                                {
                                  "id": "notifications-permission-text",
                                  "type": "p",
                                  "props": {
                                    "className": "text-xs text-muted-foreground"
                                  },
                                  "bindings": {
                                    "children": {
                                      "source": "pwaState.notificationPermission",
                                      "transform": "pwaState.copy.notifications.permissionLabel + ' ' + data"
                                    }
                                  }
                                },
                                {
                                  "id": "permission-icon-granted",
                                  "type": "CheckCircle",
                                  "conditional": {
                                    "if": "pwaState.notificationPermission === 'granted'"
                                  },
                                  "props": {
                                    "size": 16,
                                    "className": "text-accent",
                                    "weight": "fill"
                                  }
                                },
                                {
                                  "id": "permission-icon-denied",
                                  "type": "XCircle",
                                  "conditional": {
                                    "if": "pwaState.notificationPermission === 'denied'"
                                  },
                                  "props": {
                                    "size": 16,
                                    "className": "text-destructive",
                                    "weight": "fill"
                                  }
                                },
                                {
                                  "id": "permission-icon-unsupported",
                                  "type": "XCircle",
                                  "conditional": {
                                    "if": "pwaState.notificationPermission === 'unsupported'"
                                  },
                                  "props": {
                                    "size": 16,
                                    "className": "text-muted-foreground",
                                    "weight": "fill"
                                  }
                                },
                                {
                                  "id": "permission-icon-default",
                                  "type": "Question",
                                  "conditional": {
                                    "if": "pwaState.notificationPermission === 'default'"
                                  },
                                  "props": {
                                    "size": 16,
                                    "className": "text-muted-foreground",
                                    "weight": "fill"
                                  }
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "id": "notifications-switch",
                      "type": "Switch",
                      "bindings": {
                        "checked": {
                          "source": "pwaState.notificationPermission",
                          "transform": "data === 'granted'"
                        },
                        "onCheckedChange": "pwaState.handleNotificationToggle",
                        "disabled": {
                          "source": "pwaState.notificationPermission",
                          "transform": "data === 'denied' || data === 'unsupported'"
                        }
                      }
                    }
                  ]
                },
                {
                  "id": "notifications-blocked-warning",
                  "type": "div",
                  "conditional": {
                    "if": "pwaState.notificationPermission === 'denied'"
                  },
                  "props": {
                    "className": "text-xs text-destructive bg-destructive/10 p-3 rounded-md"
                  },
                  "bindings": {
                    "children": "pwaState.copy.notifications.blocked"
                  }
                },
                {
                  "id": "notifications-unsupported-warning",
                  "type": "div",
                  "conditional": {
                    "if": "pwaState.notificationPermission === 'unsupported'"
                  },
                  "props": {
                    "className": "text-xs text-muted-foreground bg-muted/50 p-3 rounded-md"
                  },
                  "bindings": {
                    "children": "pwaState.copy.notifications.unsupported"
                  }
                }
              ]
            }
          ]
        },
        {
          "id": "cache-section",
