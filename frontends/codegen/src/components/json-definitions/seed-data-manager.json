{
  "id": "seed-data-manager-card",
  "type": "Card",
  "children": [
    {
      "id": "card-header",
      "type": "CardHeader",
      "children": [
        {
          "id": "card-title",
          "type": "CardTitle",
          "children": [
            {
              "id": "title-icon",
              "type": "PhosphorIcon",
              "props": { "icon": "Database", "size": 24, "weight": "duotone" }
            },
            {
              "id": "title-text",
              "type": "text",
              "bindings": { "children": "title" }
            }
          ]
        },
        {
          "id": "card-description",
          "type": "CardDescription",
          "bindings": { "children": "description" }
        }
      ]
    },
    {
      "id": "card-content",
      "type": "CardContent",
      "props": { "className": "seed-manager__content" },
      "children": [
        {
          "id": "loaded-alert",
          "type": "Alert",
          "conditional": { "if": "isLoaded" },
          "children": [
            {
              "id": "alert-icon",
              "type": "PhosphorIcon",
              "props": { "icon": "CheckCircle", "weight": "fill" }
            },
            {
              "id": "alert-description",
              "type": "AlertDescription",
              "children": [{ "id": "alert-text", "type": "text", "children": "Seed data is loaded and available" }]
            }
          ]
        },
        {
          "id": "buttons-container",
          "type": "div",
          "props": { "className": "seed-manager__actions" },
          "children": [
            {
              "id": "buttons-row",
              "type": "div",
              "props": { "className": "seed-manager__buttons-row" },
              "children": [
                {
                  "id": "load-button",
                  "type": "Button",
                  "bindings": {
                    "onClick": "onLoadSeedData",
                    "disabled": {
                      "source": "isLoading,isLoaded",
                      "transform": "isLoading || isLoaded"
                    }
                  },
                  "props": { "variant": "default" },
                  "children": [
                    {
                      "id": "load-button-content",
                      "type": "conditional-group",
                      "conditional": {
                        "if": "isLoading",
                        "else": [
                          {
                            "id": "load-icon",
                            "type": "PhosphorIcon",
                            "props": { "icon": "Database", "size": 16, "weight": "fill" }
                          },
                          {
                            "id": "load-text",
                            "type": "text",
                            "bindings": { "children": "loadLabel" }
                          }
                        ]
                      },
                      "children": [
                        {
                          "id": "loading-spin-1",
                          "type": "PhosphorIcon",
                          "props": { "icon": "CircleNotch", "className": "animate-spin", "size": 16 }
                        },
                        {
                          "id": "loading-text-1",
                          "type": "text",
                          "bindings": { "children": "loadingLabel" }
                        }
                      ]
                    }
                  ]
                },
                {
                  "id": "reset-button",
                  "type": "Button",
                  "bindings": {
                    "onClick": "onResetSeedData",
                    "disabled": "isLoading"
                  },
                  "props": { "variant": "outline" },
                  "children": [
                    {
                      "id": "reset-button-content",
                      "type": "conditional-group",
                      "conditional": {
                        "if": "isLoading",
                        "else": [
                          {
                            "id": "reset-icon",
                            "type": "PhosphorIcon",
                            "props": { "icon": "ArrowClockwise", "size": 16, "weight": "bold" }
                          },
                          {
                            "id": "reset-text",
                            "type": "text",
                            "bindings": { "children": "resetLabel" }
                          }
                        ]
                      },
                      "children": [
                        {
                          "id": "loading-spin-2",
                          "type": "PhosphorIcon",
                          "props": { "icon": "CircleNotch", "className": "animate-spin", "size": 16 }
                        },
                        {
                          "id": "loading-text-2",
                          "type": "text",
                          "bindings": { "children": "resettingLabel" }
                        }
                      ]
                    }
                  ]
                },
                {
                  "id": "clear-button",
                  "type": "Button",
                  "bindings": {
                    "onClick": "onClearAllData",
                    "disabled": "isLoading"
                  },
                  "props": { "variant": "destructive" },
                  "children": [
                    {
                      "id": "clear-button-content",
                      "type": "conditional-group",
                      "conditional": {
                        "if": "isLoading",
                        "else": [
                          {
                            "id": "clear-icon",
                            "type": "PhosphorIcon",
                            "props": { "icon": "Trash", "size": 16, "weight": "fill" }
                          },
                          {
                            "id": "clear-text",
                            "type": "text",
                            "bindings": { "children": "clearLabel" }
                          }
                        ]
                      },
                      "children": [
                        {
                          "id": "loading-spin-3",
                          "type": "PhosphorIcon",
                          "props": { "icon": "CircleNotch", "className": "animate-spin", "size": 16 }
                        },
                        {
                          "id": "loading-text-3",
                          "type": "text",
                          "bindings": { "children": "clearingLabel" }
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "id": "helper-text",
              "type": "div",
              "props": { "className": "seed-manager__helper-text" },
              "children": [
                {
                  "id": "load-help",
                  "type": "p",
                  "conditional": { "if": "helperText.load" },
                  "children": [
                    { "id": "load-help-strong", "type": "strong", "children": "Load Seed Data: " },
                    { "id": "load-help-text", "type": "text", "bindings": { "children": "helperText.load" } }
                  ]
                },
                {
                  "id": "reset-help",
                  "type": "p",
                  "conditional": { "if": "helperText.reset" },
                  "children": [
                    { "id": "reset-help-strong", "type": "strong", "children": "Reset to Defaults: " },
                    { "id": "reset-help-text", "type": "text", "bindings": { "children": "helperText.reset" } }
                  ]
                },
                {
                  "id": "clear-help",
                  "type": "p",
                  "conditional": { "if": "helperText.clear" },
                  "children": [
                    { "id": "clear-help-strong", "type": "strong", "children": "Clear All Data: " },
                    { "id": "clear-help-text", "type": "text", "bindings": { "children": "helperText.clear" } }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
