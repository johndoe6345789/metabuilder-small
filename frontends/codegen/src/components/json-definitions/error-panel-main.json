{
  "id": "error-panel-main",
  "type": "div",
  "className": "error-panel",
  "children": [
    {
      "id": "error-panel-header-wrapper",
      "type": "MetabuilderWidgetErrorPanelHeader",
      "bindings": {
        "props": {
          "source": "headerProps",
          "transform": "data"
        }
      }
    },
    {
      "id": "error-panel-content",
      "type": "ScrollArea",
      "className": "error-panel__scroll-body",
      "children": [
        {
          "id": "error-panel-inner",
          "type": "div",
          "className": "error-panel__inner",
          "bindings": {
            "children": {
              "source": "contentState",
              "transform": "data"
            }
          }
        }
      ]
    }
  ]
}
