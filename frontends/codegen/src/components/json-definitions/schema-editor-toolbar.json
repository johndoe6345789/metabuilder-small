{
  "id": "schema-editor-toolbar",
  "type": "div",
  "props": { "className": "toolbar" },
  "children": [
    {
      "id": "toolbar-title",
      "type": "span",
      "props": { "className": "toolbar__title" },
      "children": "Schema Editor"
    },
    {
      "id": "toolbar-actions",
      "type": "div",
      "props": { "className": "toolbar__actions" },
      "children": [
        {
          "id": "import-button",
          "type": "Button",
          "props": { "variant": "outlined", "size": "sm" },
          "bindings": { "onClick": { "source": "onImport" } },
          "children": "Import"
        },
        {
          "id": "export-button",
          "type": "Button",
          "props": { "variant": "outlined", "size": "sm" },
          "bindings": { "onClick": { "source": "onExport" } },
          "children": "Export"
        },
        {
          "id": "copy-button",
          "type": "Button",
          "props": { "variant": "outlined", "size": "sm" },
          "bindings": { "onClick": { "source": "onCopy" } },
          "children": "Copy JSON"
        },
        {
          "id": "preview-button",
          "type": "Button",
          "props": { "variant": "outlined", "size": "sm" },
          "bindings": { "onClick": { "source": "onPreview" } },
          "children": "Preview"
        },
        {
          "id": "clear-button",
          "type": "Button",
          "props": { "variant": "filled", "color": "error", "size": "sm" },
          "bindings": { "onClick": { "source": "onClear" } },
          "children": "Clear"
        }
      ]
    }
  ]
}
