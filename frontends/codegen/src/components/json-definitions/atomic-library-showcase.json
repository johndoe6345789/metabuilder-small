{
  "id": "atomic-library-showcase",
  "type": "div",
  "props": {
    "className": "atomic-showcase"
  },
  "children": [
    {
      "id": "atomic-library-page-header",
      "type": "MetabuilderWidgetPageHeader",
      "bindings": {
        "title": "showcaseState.pageHeader.title",
        "description": "showcaseState.pageHeader.description"
      }
    },
    {
      "id": "atomic-library-sections-stack",
      "type": "div",
      "props": {
        "className": "atomic-showcase__sections"
      },
      "children": [
        {
          "id": "atomic-library-buttons-actions",
          "type": "ButtonsActionsSection",
          "bindings": {
            "content": "showcaseState.sections.buttonsActions"
          }
        },
        {
          "id": "atomic-library-badges-indicators",
          "type": "BadgesIndicatorsSection",
          "bindings": {
            "content": "showcaseState.sections.badgesIndicators"
          }
        },
        {
          "id": "atomic-library-typography",
          "type": "TypographySection",
          "bindings": {
            "content": "showcaseState.sections.typography"
          }
        },
        {
          "id": "atomic-library-form-controls",
          "type": "FormControlsSection",
          "bindings": {
            "content": "showcaseState.sections.formControls",
            "switchChecked": "showcaseState.switchChecked",
            "onSwitchChange": "showcaseState.setSwitchChecked",
            "selectedDate": "showcaseState.selectedDate",
            "onDateChange": "showcaseState.setSelectedDate",
            "filterValue": "showcaseState.filterValue",
            "onFilterChange": "showcaseState.setFilterValue",
            "rating": "showcaseState.rating",
            "onRatingChange": "showcaseState.setRating",
            "rangeValue": "showcaseState.rangeValue",
            "onRangeChange": "showcaseState.setRangeValue"
          }
        },
        {
          "id": "atomic-library-progress-loading",
          "type": "ProgressLoadingSection",
          "bindings": {
            "content": "showcaseState.sections.progressLoading"
          }
        },
        {
          "id": "atomic-library-feedback",
          "type": "FeedbackSection",
          "bindings": {
            "content": "showcaseState.sections.feedback"
          }
        },
        {
          "id": "atomic-library-avatars-user-elements",
          "type": "AvatarsUserElementsSection",
          "bindings": {
            "content": "showcaseState.sections.avatarsUserElements"
          }
        },
        {
          "id": "atomic-library-cards-metrics",
          "type": "CardsMetricsSection",
          "bindings": {
            "content": "showcaseState.sections.cardsMetrics"
          }
        },
        {
          "id": "atomic-library-interactive-elements",
          "type": "InteractiveElementsSection",
          "bindings": {
            "content": "showcaseState.sections.interactiveElements"
          }
        },
        {
          "id": "atomic-library-layout-components",
          "type": "LayoutComponentsSection",
          "bindings": {
            "content": "showcaseState.sections.layoutComponents"
          }
        },
        {
          "id": "atomic-library-enhanced-components",
          "type": "EnhancedComponentsSection",
          "bindings": {
            "content": "showcaseState.sections.enhancedComponents",
            "numberValue": "showcaseState.numberValue",
            "onNumberChange": "showcaseState.setNumberValue"
          }
        },
        {
          "id": "atomic-library-summary",
          "type": "SummarySection",
          "bindings": {
            "content": "showcaseState.sections.summary"
          }
        }
      ]
    }
  ]
}
