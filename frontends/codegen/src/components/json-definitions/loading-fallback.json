{
  "id": "loading-container",
  "type": "div",
  "props": {
    "className": "loading-fallback"
  },
  "children": [
    {
      "id": "loading-content",
      "type": "div",
      "props": {
        "className": "loading-fallback__content"
      },
      "children": [
        {
          "id": "loading-spinner",
          "type": "LoadingSpinner"
        },
        {
          "id": "loading-message",
          "type": "p",
          "props": {
            "className": "loading-fallback__message"
          },
          "bindings": {
            "children": "message"
          }
        }
      ]
    }
  ]
}
