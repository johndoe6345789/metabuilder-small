{
  "id": "documentation-view",
  "type": "div",
  "className": "doc-view",
  "children": [
    {
      "id": "documentation-tabs",
      "type": "Tabs",
      "className": "doc-view__tabs",
      "bindings": {
        "value": {
          "source": "activeTab",
          "transform": "data"
        },
        "onValueChange": {
          "source": "setActiveTab",
          "transform": "data"
        }
      },
      "children": [
        {
          "id": "documentation-header",
          "type": "div",
          "className": "doc-view__header",
          "children": [
            {
              "id": "tabs-list",
              "type": "TabsList",
              "bindings": {
                "children": {
                  "source": "tabsData",
                  "transform": "data"
                }
              }
            },
            {
              "id": "search-input-wrapper",
              "type": "div",
              "className": "doc-view__search-wrapper",
              "children": [
                {
                  "id": "search-icon",
                  "type": "MagnifyingGlass",
                  "className": "doc-view__search-icon",
                  "size": 18
                },
                {
                  "id": "search-input",
                  "type": "Input",
                  "placeholder": "Search documentation...",
                  "className": "doc-view__search-input",
                  "bindings": {
                    "value": {
                      "source": "searchQuery",
                      "transform": "data"
                    },
                    "onChange": {
                      "source": "handleSearchChange",
                      "transform": "data"
                    }
                  }
                }
              ]
            }
          ]
        },
        {
          "id": "tabs-content",
          "type": "ScrollArea",
          "className": "doc-view__body",
          "children": [
            {
              "id": "content-wrapper",
              "type": "div",
              "className": "doc-view__content",
              "bindings": {
                "children": {
                  "source": "activeTab",
                  "transform": "data"
                }
              }
            }
          ]
        }
      ]
    }
  ]
}
