{
  "id": "seed-data-status-card",
  "type": "Card",
  "bindings": {
    "className": {
      "source": "className",
      "transform": "data || ''"
    }
  },
  "children": [
    {
      "id": "seed-data-status-header",
      "type": "CardHeader",
      "children": [
        {
          "id": "seed-data-status-title",
          "type": "CardTitle",
          "props": {
            "className": "seed-data-status__title"
          },
          "children": [
            {
              "id": "seed-data-status-icon",
              "type": "Database",
              "bindings": {
                "size": {
                  "source": null,
                  "transform": "20"
                },
                "weight": {
                  "source": null,
                  "transform": "'duotone'"
                }
              }
            },
            {
              "id": "seed-data-status-text",
              "type": "span",
              "bindings": {
                "children": {
                  "source": null,
                  "transform": "'Seed Data Available'"
                }
              }
            }
          ]
        },
        {
          "id": "seed-data-status-description",
          "type": "CardDescription",
          "bindings": {
            "children": {
              "source": null,
              "transform": "'Pre-configured data ready to load from database'"
            }
          }
        }
      ]
    },
    {
      "id": "seed-data-status-content",
      "type": "CardContent",
      "children": [
        {
          "id": "seed-data-status-grid",
          "type": "div",
          "props": {
            "className": "seed-data-status__grid"
          },
          "children": [
            {
              "id": "seed-data-status-item",
              "type": "div",
              "props": {
                "className": "seed-data-status__item"
              },
              "children": [
                {
                  "id": "seed-data-status-label",
                  "type": "span",
                  "props": {
                    "className": "seed-data-status__label"
                  },
                  "bindings": {
                    "children": {
                      "source": "label",
                      "transform": "data"
                    }
                  }
                },
                {
                  "id": "seed-data-status-badge",
                  "type": "Badge",
                  "props": {
                    "className": "seed-data-status__badge"
                  },
                  "bindings": {
                    "variant": {
                      "source": null,
                      "transform": "'secondary'"
                    },
                    "children": {
                      "source": "count",
                      "transform": "data"
                    }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
