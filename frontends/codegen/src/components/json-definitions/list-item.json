{
  "id": "list-item-container",
  "type": "div",
  "bindings": {
    "className": {
      "source": ["onClick", "active", "className"],
      "transform": "const onClick = data[0]; const active = data[1]; const customClass = data[2] || ''; const interactiveClass = onClick ? 'list-item--interactive' : ''; const activeClass = active ? 'list-item--active' : ''; return ['list-item', interactiveClass, activeClass, customClass].filter(Boolean).join(' ')"
    },
    "onClick": {
      "source": "onClick",
      "transform": "data"
    },
    "role": {
      "source": "onClick",
      "transform": "data ? 'button' : undefined"
    },
    "tabIndex": {
      "source": "onClick",
      "transform": "data ? 0 : undefined"
    }
  },
  "children": [
    {
      "id": "list-item-icon",
      "type": "div",
      "props": {
        "className": "list-item__icon"
      },
      "bindings": {
        "children": {
          "source": "icon",
          "transform": "data"
        },
        "_if": {
          "source": "icon",
          "transform": "!!data"
        }
      }
    },
    {
      "id": "list-item-content",
      "type": "div",
      "props": {
        "className": "list-item__content"
      },
      "bindings": {
        "children": {
          "source": "children",
          "transform": "data"
        }
      }
    },
    {
      "id": "list-item-end-content",
      "type": "div",
      "props": {
        "className": "list-item__end"
      },
      "bindings": {
        "children": {
          "source": "endContent",
          "transform": "data"
        },
        "_if": {
          "source": "endContent",
          "transform": "!!data"
        }
      }
    }
  ]
}
