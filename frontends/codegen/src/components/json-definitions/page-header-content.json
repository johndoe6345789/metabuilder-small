{
  "id": "page-header-container",
  "type": "div",
  "props": { "className": "page-header-content" },
  "children": [
    {
      "id": "page-header-icon",
      "type": "TabIcon",
      "bindings": { "icon": "icon" },
      "props": { "variant": "gradient" }
    },
    {
      "id": "page-header-text",
      "type": "div",
      "props": { "className": "page-header-content__text" },
      "children": [
        {
          "id": "page-header-title",
          "type": "h2",
          "props": { "className": "page-header-content__title" },
          "bindings": { "children": "title" }
        },
        {
          "id": "page-header-description",
          "type": "p",
          "props": { "className": "page-header-content__description" },
          "bindings": { "children": "description" },
          "conditional": { "if": "description" }
        }
      ]
    }
  ]
}
