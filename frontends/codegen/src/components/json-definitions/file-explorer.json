{
  "id": "file-explorer-container",
  "type": "div",
  "props": {
    "className": "file-explorer"
  },
  "children": [
    {
      "id": "file-explorer-header",
      "type": "div",
      "props": {
        "className": "file-explorer__header"
      },
      "children": [
        {
          "id": "file-explorer-title",
          "type": "h3",
          "props": {
            "className": "file-explorer__title"
          },
          "children": [
            {
              "id": "folder-icon",
              "type": "FolderOpen",
              "props": {
                "size": 18,
                "weight": "duotone"
              }
            },
            "Files"
          ]
        },
        {
          "id": "file-explorer-dialog",
          "type": "FileExplorerDialog",
          "bindings": {
            "onFileAdd": "onFileAdd"
          }
        }
      ]
    },
    {
      "id": "file-explorer-list",
      "type": "ScrollArea",
      "props": {
        "className": "file-explorer__body"
      },
      "children": [
        {
          "id": "file-explorer-items",
          "type": "FileExplorerList",
          "bindings": {
            "files": "files",
            "activeFileId": "activeFileId",
            "onFileSelect": "onFileSelect"
          }
        }
      ]
    }
  ]
}
