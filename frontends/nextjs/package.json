{
  "name": "metabuilder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "mkdir -p ../../node_modules/.prisma/client && echo 'module.exports={PrismaClient:function(){return{}}}' > ../../node_modules/.prisma/client/default.js && echo 'module.exports={}' > ../../node_modules/.prisma/client/index-browser.js",
    "build": "next build --webpack",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test --config=../../playwright.config.ts",
    "test:e2e:ui": "playwright test --config=../../playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=../../playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=../../playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:dbal-daemon": "playwright test --config=../../e2e/playwright.dbal-daemon.config.ts"
  },
  "dependencies": {
    "@metabuilder/api-clients": "*",
    "@metabuilder/core-hooks": "*",
    "@metabuilder/hooks": "*",
    "@metabuilder/redux-persist": "*",
    "@monaco-editor/react": "^4.7.0",
    "@octokit/core": "^7.0.6",
    "better-sqlite3": "^12.6.2",
    "clsx": "^2.1.1",
    "esbuild": "^0.27.3",
    "jsdom": "^28.1.0",
    "next": "^16.1.6",
    "octokit": "^5.0.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "server-only": "^0.0.1",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "overrides": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "@chromatic-com/playwright": "^0.12.8",
    "checkly": "^7.1.0",
    "npm-run-all2": "8.0.4",
    "null-loader": "^4.0.1",
    "@sentry/nextjs": "^10.39.0",
    "@spotlightjs/spotlight": "^4.10.0"
  }
}
