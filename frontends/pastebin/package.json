{
  "name": "codesnippet",
  "version": "0.3.8",
  "type": "module",
  "private": true,
  "scripts": {
    "copy:pyodide": "node -e \"const{mkdirSync,copyFileSync}=require('fs');const{join,resolve}=require('path');const src=resolve(__dirname,'../../node_modules/pyodide');const dst=resolve(__dirname,'public/pyodide');mkdirSync(dst,{recursive:true});['pyodide.js','pyodide.mjs','pyodide.asm.js','pyodide.asm.wasm','python_stdlib.zip','pyodide-lock.json'].forEach(f=>copyFileSync(join(src,f),join(dst,f)));console.log('Pyodide files copied to public/pyodide/');\"",
    "dev": "next dev --turbopack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint . --config eslint.config.mjs",
    "lint:fix": "eslint . --config eslint.config.mjs --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:unit": "jest --coverage",
    "quality-check": "node run-quality-check.mjs",
    "quality-check:json": "node run-quality-check.mjs --format json --output .quality/report.json",
    "quality-check:html": "node run-quality-check.mjs --format html --output .quality/report.html",
    "quality-check:verbose": "node run-quality-check.mjs --verbose"
  },
  "dependencies": {
    "@babel/standalone": "^7.29.1",
    "@metabuilder/components": "file:../../components",
    "@metabuilder/hooks": "file:../../hooks",
    "@metabuilder/redux-persist": "file:../../redux/persist",
    "@metabuilder/services": "file:../../redux/services",
    "@monaco-editor/react": "^4.7.0",
    "@phosphor-icons/react": "^2.1.10",
    "@reduxjs/toolkit": "^2.11.2",
    "class-variance-authority": "^0.7.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.34.3",
    "idb": "^8.0.3",
    "lucide-react": "^0.575.0",
    "monaco-editor": "^0.55.1",
    "next": "^16.1.6",
    "pyodide": "^0.29.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.1",
    "react-hook-form": "^7.71.1",
    "react-redux": "^9.2.0",
    "react-resizable-panels": "^4.6.4",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "sucrase": "^3.35.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.24",
    "eslint": "^10.0.1",
    "eslint-config-next": "^16.1.6",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "sass": "^1.97.3",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.56.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "@chromatic-com/playwright": "^0.12.8",
    "checkly": "^7.1.0",
    "npm-run-all2": "8.0.4",
    "null-loader": "^4.0.1",
    "@sentry/nextjs": "^10.39.0",
    "@spotlightjs/spotlight": "^4.10.0"
  },
  "overrides": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  }
}
